            @*@Html.ActionLink("ToKil", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@


            <div class="navbar-collapse collapse">
                <ul class="nav navbar-left">
                    <li>@Html.ActionLink("Accueil", "Index", "Home")</li>
                </ul>
                @Html.Partial("_loginPartial")
